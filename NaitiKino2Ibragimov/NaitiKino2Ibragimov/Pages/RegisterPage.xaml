<Page x:Class="NaitiKino2Ibragimov.Pages.RegisterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NaitiKino2Ibragimov.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterPage">

    <Grid>
        <StackPanel>
            <TextBlock Text="ФИО"/>
            <TextBox x:Name="TBFIO"/>
            <TextBlock Text="Ник"/>
            <TextBox x:Name="TBNick"/>
            <TextBlock Text="Аватар"/>
            <TextBox x:Name="TBAvatar"/>
            <TextBlock Text="Логин"/>
            <TextBox x:Name="TBLogin"/>
            <TextBlock Text="Пароль"/>
            <TextBox x:Name="TBPassword"/>
            <Button x:Name="BRegister"
                    Content="Регистрация"
                    Click="BRegister_Click"/>
        </StackPanel>
    </Grid>
</Page>
